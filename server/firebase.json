{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions",
    "ignore": [
      "**/node_modules/**",
      "localTrigger.js",
      "**.log",
      ".eslintrc.js",
      ".gitignore",
      "package-lock.json"
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "database": {
      "port": 9000
    },
    "pubsub": {
      "port": 8085
    },
    "ui": {
      "enabled": true
    }
  }
}
