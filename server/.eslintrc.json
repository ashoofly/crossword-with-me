{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "max-len": ["error", { "code": 105 }],
      "no-console": "off",
      "no-underscore-dangle": "off",
      "function-paren-newline": "off",
      "one-var": "off",
      "one-var-declaration-per-line": "off"
    }
}
